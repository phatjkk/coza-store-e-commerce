                    @{
                        int tongTien = 0;
                        int sl;
                        foreach (SanPham k in ((KhachSession)Session["ttKhach"]).gioHang)
                        {

                            sl = ((KhachSession)Session["ttKhach"]).listSLSP.Where(m => m.masp == k.maSP).ToList().First().sl;
